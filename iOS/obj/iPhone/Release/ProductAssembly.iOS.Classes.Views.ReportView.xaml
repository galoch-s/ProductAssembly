<?xml version="1.0" encoding="UTF-8"?>
<ContentView 
		xmlns="http://xamarin.com/schemas/2014/forms" 
		xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
		x:Class="ProductAssembly.ReportView"
		xmlns:xlabs="clr-namespace:XLabs.Forms.Controls"
		xmlns:cells="clr-namespace:ProductAssembly" VerticalOptions="FillAndExpand">
	<ContentView.Content>
		<xlabs:GridView
			x:Name="ContainerGridView"
			RowSpacing="5"
			ColumnSpacing="5"
			ContentPaddingBottom="0"
			ContentPaddingTop="6"
			ContentPaddingLeft="0"
			ContentPaddingRight="0"
			ItemWidth="180"
			ItemHeight="60"
			ItemSelected="OnItemSelected"
			SelectionEnabled="true">

			<!--<xlabs:GridView.ItemTemplate>
				<DataTemplate>
					<cells:ContainerCell EventLogin="{x:Static Control:Constants.HeightRowProduct}" />
				</DataTemplate>
			</xlabs:GridView.ItemTemplate>-->

		</xlabs:GridView>
	</ContentView.Content>
</ContentView>
